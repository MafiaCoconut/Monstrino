{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "color": {
      "()": "colorlog.ColoredFormatter",
      "format": "%(log_color)s%(asctime)s | %(levelname)-8s | %(name)-40s | %(message)s",
      "datefmt": "%d.%m.%Y-%H:%M",
      "log_colors": {
        "DEBUG": "cyan",
        "INFO": "green",
        "WARNING": "yellow",
        "ERROR": "red",
        "CRITICAL": "bold_red"
      }
    },
    "default": {
      "format": "%(asctime)s | %(levelname)-8s | %(name)-40s | %(message)s",
      "datefmt": "%d.%m.%Y-%H:%M"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "color",
      "level": "DEBUG",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "uvicorn": {
      "handlers": ["console"],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.error": {
      "handlers": ["console"],
      "level": "ERROR",
      "propagate": false
    },
    "uvicorn.access": {
      "handlers": ["console"],
      "level": "INFO",
      "propagate": false
    },
    "aiokafka.conn": {
      "handlers": ["console"],
      "level": "INFO",
      "propagate": false
    },
    "aiokafka.consumer.fetcher": {
      "handlers": [
        "console"
      ],
      "level": "INFO",
      "propagate": false
    },
    "aiokafka.consumer.group_coordinator": {
      "handlers": [
        "console"
      ],
      "level": "INFO",
      "propagate": false
    },
    "aiokafka.cluster": {
      "handlers": ["console"],
      "level": "INFO",
      "propagate": false
    },
    "watchfiles.main": {
      "handlers": ["console"],
      "level": "ERROR",
      "propagate": false
    }

    },
  "root": {
    "handlers": ["console"],
    "level": "DEBUG"
  }
}